

<div class="container container-projects">
    {{#if projects}}
    {{#each projects}}
    <div class="jumbotron">
        <h1>{{name}}</h1>
        <p>{{{description}}}</p>
        <a class="btn btn-primary" href="{{url}}" target="_blank" role="button">See it here</a>
    </div>
    {{/each}}
    {{else}}
    Could not find any projects.
    {{/if}}
</div>